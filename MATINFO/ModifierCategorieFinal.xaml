<Window x:Class="MATINFO.ModifierCategorieFinal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MATINFO"
        mc:Ignorable="d"
        Title="ModifierCategorieFinal" Height="170" Width="300" MinHeight=" 170" MinWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="Nouveau nom :" HorizontalAlignment="Left" Margin="15,39,0,0" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="tbModifierCategorieFinal" HorizontalAlignment="Left" Margin="109,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Height="50" Grid.ColumnSpan="2"/>
        <Button Content="Modifier" HorizontalAlignment="Left" Margin="106,102,0,0" VerticalAlignment="Top" Height="32" Width="88" Grid.ColumnSpan="2">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="10" />
                </Style>
            </Button.Resources>
        </Button>

    </Grid>
</Window>
